            function getMessageNotiData() {
                var $noti = $('#messageNoti')
                var $totalNofi = $('#messTotalNoti')
                $.ajax({
                    url: location.origin + "/Chat/GetNotiMessage",
                    type: 'GET',
                    dataType: 'JSON',
                    success: function (data) {
                        $noti.empty()
                        $totalNofi.empty()
                        if (data.length > 0) {
                            $totalNofi.append(
                                data.length
                            )
                            $.each(data, function (i, model) {
                                $noti.append(
                                    '<li>' +
                                    '<a href = "/Chat/Chating?WithUserID=' + model.FromUserID + '&ChatId=' + model.ID + '" class= "dropdown-item" id="' + model.FromUserID + '">' +
                                    '<div class="alert alert-info clearfix">' +
                                    '<span class="alert-icon"><i class="fa fa-bolt"></i></span>' +
                                    '  <div class="noti-info">' +
                                    '<p>' + model.FromUserName + '</p>' +
                                    '<p class="text-sm text-muted"><i class="far fa-clock mr-1"></i>' + model.CreatedDate + ' phút trước</p>' +
                                    ' </div>' +
                                    '   </div>' +
                                    ' </li>'
                                );
                            })
                        } else {
                            $totalNofi.append(
                                0
                            )
                            $noti.append(
                                '<a href="/Chat/" class="dropdown-item dropdown-footer">Xem tất cả tin nhắn</a>'
                            );
                        }
                    }
                })
            }
