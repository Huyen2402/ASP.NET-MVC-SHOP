<script>
    $(".luuKM").click(function () {
        var idUser = $("#IdUser").val();
        console.log(idUser)
        if (idUser != "") {
            var maGiamGia = $(this).attr("data-KM");
            


            
            $.ajax({
                type: 'GET',
                dataType: 'JSON',
                url: "http://localhost:62979/SanPham/LuuGiamGia?MaGiamGia=" + maGiamGia,
                success: function (status) {
                    if (status) {
                        Swal.fire({
                            icon: 'success',
                            title: 'Thành công',
                            text: 'Nhận voucher thành công',

                        })
                    }
                }

            })
        }
        else {
            Swal.fire({
                icon: 'error',
                title: 'Có lỗi',
                text: 'Vui lòng đăng nhập để nhận vourcher',

            })

        }

    })
</script>